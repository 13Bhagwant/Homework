<%- include("../partials/header") %>
<form class="mt-4" action="/cohorts" method="POST">
  <div class="form-group">
    <label for="logoUrl">Logo Url</label>
    <input type="text" class="form-control" name="logoUrl" id="logoUrl" placeholder=" https://Anything/.png">
  </div>
  <div class="form-group">
    <label for="name">Name</label>
    <input type="text" class="form-control" name="name" id="name" placeholder="Name of the Cohort">
  </div>
  <div class="form-group">
  <label for="members">Members</label>
  <textarea class="form-control members-field" name="members" id="members" placeholder="Name of the Members" rows="3"></textarea>
  <div class="align-right">
  
  <button type="submit" class="btn btn-outline-primary mt-4 neww-team-button">Submit</button>
  </div>
</div>
</form>
<%- include("../partials/footer") %>